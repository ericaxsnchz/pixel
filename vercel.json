{ 
    "version": 2, 
    "builds": [
        {
            "src": "server/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server/app.js"
        }
    ],
    "functions": {
        "api/*": {
          "runtime": "nodejs14.x",
          "timeout": 30
        }
      }
}